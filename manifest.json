{ 
  "manifest_version": 2,
  "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },

"commands":{
    "play-pause-video":{
      "description":"Play/Pause Video",
      "suggested_key": {
              "default": "Ctrl+Period",
              "windows": "Ctrl+Period",
              "mac": "Command+Period"
            }
    },
    "play-next-video":{
      "description":"Play Next Video",
      "suggested_key": {
              "default": "Ctrl+Right",
              "windows": "Ctrl+Right",
              "mac": "Command+Right"
            }
    },
    "play-previous-video":{
      "description":"Play Previous Video",
      "suggested_key": {
              "default": "Ctrl+Left",
              "windows": "Ctrl+Left",
              "mac": "Command+Left"
            }
    }
  },

"content_scripts": [ {
      "js": [ "onYoutube.js","social.js" ],
      "css":["css/onYoutube.css","css/font-awesome.min.css"],
      "matches": [ "http://www.youtube.com/*", "https://www.youtube.com/*" ],
      "run_at": "document_end"
   }],
  "name": "qTube - Queue Youtube\u2122 Videos",
  "short_name": "qTube",
  "description": "Get more out of Youtube, use it as a music player! qTube allows u to build temporary youtube video queue",
  "version": "1.7",
"icons": {
      "128": "icon.png",
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png"
   },
 "web_accessible_resources": [ "right.png","icon.png","img/progress.gif"],
"permissions": [
   
	"tabs","http://*.youtube.com/*","https://*.youtube.com/*","contextMenus","notifications"
]	
}
